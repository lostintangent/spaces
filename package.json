    "configuration": {
      "type": "object",
      "title": "Live Share Spaces",
      "properties": {
        "liveshare.spaces.showSuggestedContacts": {
          "default": false,
          "type": "boolean",
          "description": "Specifies whether to show space-contributed contacts in the Live Share Suggested Contacts tree."
        },
        "liveshare.spaces.extensionSuggestionBehavior": {
          "default": "join",
          "type": "string",
          "enum": [
            "ignore",
            "join",
            "prompt"
          ],
          "description": "Specifies how to respond when 3rd-party extensions declare they have an associated space."
        },
        "liveshare.spaces.workspaceSuggestionBehavior": {
          "default": "join",
          "type": "string",
          "enum": [
            "ignore",
            "join",
            "prompt"
          ],
          "description": "Specifies how to respond when a workspace declares they have an associated space."
        },
        "liveshare.spaces.mutedSpaces": {
          "default": [],
          "type": "array",
          "description": "Specifies which spaces to mute notifications for (specify * to mute all)."
        },
        "liveshare.spaces.serviceUri": {
          "default": "prod",
          "type": "string",
          "enum": [
            "prod",
            "local"
          ],
          "description": "Specifies the services endpoint URI"
        }
      }
    },
